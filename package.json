{
  "name": "nuxt-boilerplate",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "private": true,
  "scripts": {
    "dashboard": "nuxt -c ./build/config.dashboard.js",
    "manager": "nuxt -c ./build/config.manager.js",
    "dev:d": "node build/index.js && npm run dashboard",
    "dev:m": "node build/index.js && npm run manager",
    "build:d": "nuxt build -c ./build/config.dashboard.js",
    "build:m": "nuxt build -c ./build/config.manager.js",
    "build": "npm run build:d && npm run build:m",
    "start:d": "nuxt start -c ./build/config.dashboard.js -p 3000",
    "start:m": "nuxt start -c ./build/config.manager.js -p 3001"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/style-resources": "^1.0.0",
    "@nuxtjs/vuetify": "^1.11.2",
    "chart.js": "^2.9.3",
    "nuxt": "^2.0.0",
    "throttle-debounce": "^2.2.1",
    "vue-chartjs": "^3.5.0",
    "vue-select": "^3.10.3"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/svg": "^0.1.11",
    "babel-eslint": "^10.1.0",
    "chalk": "^4.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "fs-extra": "^9.0.1",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "sass-loader": "^8.0.2",
    "webpack": "^4.43.0"
  }
}
