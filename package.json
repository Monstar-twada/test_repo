{
  "name": "nuxt-boilerplate",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "private": true,
  "scripts": {
    "dashboard": "nuxt -c ./build/config.dashboard.js",
    "manager": "nuxt -c ./build/config.manager.js",
    "console": "nuxt -c ./build/config.console.js",
    "dev:d": "npm run dashboard",
    "dev:m": "npm run manager",
    "dev:c": "npm run console",
    "build:d": "nuxt build -c ./build/config.dashboard.js",
    "build:m": "nuxt build -c ./build/config.manager.js",
    "build:c": "nuxt build -c ./build/config.console.js",
    "build": "npm run build:d && npm run build:m && npm run build:c",
    "start:d": "nuxt start -c ./build/config.dashboard.js -p 3000",
    "start:m": "nuxt start -c ./build/config.manager.js -p 3001",
    "start:c": "nuxt start -c ./build/config.console.js -p 3002",
    "ui:prefix": "node plugins/first-group-ui/build/index.js",
    "ui": "npm run ui:prefix && webpack-dev-server --config plugins/first-group-ui/build/webpack/dev.config.js"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/style-resources": "^1.0.0",
    "@nuxtjs/vuetify": "^1.11.2",
    "async-validator": "^3.4.0",
    "chart.js": "^2.9.3",
    "deepmerge": "^4.2.2",
    "image-process": "^4.0.0",
    "nuxt": "^2.0.0",
    "throttle-debounce": "^2.2.1",
    "vue-chartjs": "^3.5.0",
    "zx-calendar": "^0.5.3"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/svg": "^0.1.11",
    "babel-eslint": "^10.1.0",
    "chalk": "^4.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "fs-extra": "^9.0.1",
    "markdown-it": "^11.0.0",
    "markdown-it-multimd-table": "^4.0.3",
    "node-sass": "^4.14.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "progress-bar-webpack-plugin": "^2.1.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.3"
  }
}
